<!DOCTYPE html>

<title>Historique</title>

{% block body %}
    <h1>Historique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ historique.id }}</td>
            </tr>
            <tr>
                <th>Date_evenement</th>
                <td>{{ historique.dateEvenement ? historique.dateEvenement|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Type_evenement</th>
                <td>{{ historique.typeEvenement }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ historique.description }}</td>
            </tr>
            <tr>
                <th>Quantite_dechets</th>
                <td>{{ historique.quantiteDechets }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_historique_index') }}">back to list</a>

    <a href="{{ path('app_historique_edit', {'id': historique.id}) }}">edit</a>

    {{ include('historique/_delete_form.html.twig') }}
{% endblock %}
